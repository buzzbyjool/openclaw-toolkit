<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>com.openclaw.exec-guardian</string>
  <key>ProgramArguments</key>
  <array>
    <string>/usr/bin/python3</string>
    <string>/path/to/your/home/.openclaw/guardian/exec-guardian.py</string>
  </array>
  <key>RunAtLoad</key>
  <true/>
  <key>KeepAlive</key>
  <true/>
  <key>StandardOutPath</key>
  <string>/path/to/your/home/.openclaw/guardian/logs/guardian-launchd.log</string>
  <key>StandardErrorPath</key>
  <string>/path/to/your/home/.openclaw/guardian/logs/guardian-launchd.log</string>
</dict>
</plist>
